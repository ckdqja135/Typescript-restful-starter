{
  "name": "typescript-restful-starter",
  "version": "0.1.0",
  "description": "kit starter node application",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && cd dist && cross-env NODE_ENV=PRODUCTION node -r dotenv/config Index.js",
    "dev": "nodemon",
    "test": "cross-env NODE_ENV=test mocha test/**/*.ts",
    "build": "npm run clean && tsc --outDir dist && cp .env ./dist",
    "clean": "rimraf dist"
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "typescript",
    "tslint",
    "JWT",
    "E2015"
  ],
  "author": "Hector Riquelme",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/method-override": "0.0.31",
    "@types/node": "^12.7.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.10.0",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "superagent": "^5.1.0",
    "token-extractor": "^0.1.6",
    "ts-node": "^8.3.0",
    "typeorm": "^0.2.24",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/cors": "^2.8.6",
    "@types/express-jwt": "0.0.42",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/rimraf": "2.0.2",
    "@types/superagent": "^4.1.3",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "tslint": "^5.19.0"
  }
}
